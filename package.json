    "ava": "*",
    "babel": "^5.8.23",
    "cookie": "^0.1.2",
    "coveralls": "^2.11.2",
    "get-stream": "^1.0.0",
    "image-size": "^0.3.3",
    "nyc": "^4.0.1",
    "path-exists": "^2.0.0",
    "png-js": "^0.1.1",
    "rfpify": "^0.1.0",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "ignores": [
      "dist/**",
      "lib/index.js",
      "test/_*.js"
