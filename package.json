    "snapshot",
    "shot",
    "responsive",
    "gulpfriendly",
    "phantom",
    "phantomjs",
    "image",
    "svg",
    "render",
    "html",
    "headless",
    "capture",
    "pic",
    "picture",
    "png",
    "jpg",
    "jpeg"
  ],
  "dependencies": {
    "array-differ": "^1.0.0",
    "array-uniq": "^1.0.2",
    "babel-runtime": "^5.8.29",
    "easydate": "^2.0.0",
    "es5-shim": "^4.0.3",
    "filenamify-url": "^1.0.0",
    "fs-write-stream-atomic": "^1.0.2",
    "get-res": "^1.0.0",
